{
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "blink",
  "editor.fontFamily": "Fira Code iScript, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "on",
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": true,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.useTabStops": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.openEditors.visible": 1,
  "screencastMode.onlyKeyboardShortcuts": true,
  "terminal.integrated.fontSize": 18,
  "workbench.activityBar.visible": true,
  "materialTheme.accent": "Yellow",
  "workbench.preferredDarkColorTheme": "Material Gecko",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.linkedEditing": true,
  "editor.minimap.scale": 2,
  "prettier.enable": true,
  "eslint.alwaysShowStatus": true,
  "liveServer.settings.CustomBrowser": "chrome",
  "peacock.affectPanelBorder": true,
  "workbench.colorCustomizations": {
    "[eppz!]": {},
    "activityBar.activeBackground": "#66ccf3",
    "activityBar.background": "#66ccf3",
    "panel.border": "#66ccf3",
    "statusBar.background": "#37bcef",
    "statusBarItem.hoverBackground": "#12a8e1",
    "statusBarItem.remoteBackground": "#37bcef",
    "sash.hoverBorder": "#66ccf3",
    "titleBar.activeBackground": "#37bcef",
    "titleBar.inactiveBackground": "#37bcef99",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99",
    "statusBar.foreground": "#15202b",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ec13b0",
    "activityBarBadge.foreground": "#e7e7e7"
  },
  "workbench.colorTheme": "Default Dark Modern",
  "vscode-color-picker.languages": [
    "python",
    "javascript",
    "typescript",
    "html",
    "css"
  ],
  "pieces.autocomplete": true,
  "pieces.autocompleteKey": "Tab only",
  "pieces.display": "Preview",
  "eslint.codeActionsOnSave.rules": null,
  "peacock.affectEditorGroupBorder": false,
  "peacock.color": "#37bcef",
  "editor.codeActionsOnSave": {},
  "keyboard.touchbar.enabled": false,
  "color-highlight.markRuler": true,
  "editor.acceptSuggestionOnEnter": "off"
}
